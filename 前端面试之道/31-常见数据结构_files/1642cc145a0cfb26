<template>
  <div class="login-page">
    <div class="max-container">
      <div class="logo-img">
        <img src="../assets/logo.png" alt />
      </div>
      <div class="wel-title">欢迎登录</div>
      <div class="input-container">
        <input type="text" placeholder="请输入账号" class="username" v-model="username" />
      </div>

      <div class="input-container">
        <input type="password" placeholder="请输入密码" class="password" v-model="password" />
      </div>

      <div class="forget-container">
        <div class="forget-list">
          <div @click="goPassword">忘记密码</div>
          <div @click="goRegistered">立即注册</div>
        </div>
      </div>

      <div class="login-btn">
        <button @click="goLogin">登录</button>
      </div>
    </div>
  </div>
</template>

<script>
import { Toast } from "vant";
export default {
  name: "LoginPage",
  props: {},
  data() {
    return {
      devid: sessionStorage.getItem("devid") || "",
      username: "",
      password: "",
      showContainer: true
    };
  },
  created() {
    this.getDevid();
  },
  methods: {
    //创建设备的devid值
    getDevid() {
      if (this.devid) {
        return;
      } else {
        let time = new Date().getTime();
        let ua = navigator.userAgent;
        let devid = this.$md5(time + ua);
        sessionStorage.setItem("devid", devid);
        this.devid = devid;
      }
    },
    //注册
    goRegistered() {
      this.$router.push({
        path: "/registered",
        query: {
          type: 1
        }
      });
    },
    //忘记密码
    goPassword() {
      this.$router.push({
        path: "/registered",
        query: {
          type: 2
        }
      });
    },
    //登录
    goLogin() {
      let that = this;
      if (!that.username) {
        Toast("请输入登录账号");
        return;
      }
      if (!that.password) {
        Toast("请输入登录密码");
        return;
      } else {
        const toast = Toast.loading({
          duration: 0, // 持续展示 toast
          forbidClick: true,
          message: "登录中..."
        });
        that.$axios
          .post(
            "/portal/user/login",
            that.$qs.stringify({
              devid: that.devid,
              username: that.username,
              country: "86",
              password: that.password,
              telphone: that.username
            })
          )
          .then(res => {
            console.log(res);
            if (res.data.code == 200) {
              Toast.clear();
              sessionStorage.setItem("token", res.data.data.token);
              sessionStorage.setItem("username", res.data.data.username);
              this.$router.push({
                path: "/home"
              });
            } else {
              Toast.clear();
              Toast(res.data.message);
            }
          });
      }
    }
  }
};
</script>

